  "version": "0.2.0",
  "configurations": [
    {
			"name": "调试程序",
      "type": "node",
      "request": "launch",
			"cwd": "${workspaceRoot}",
			"restart": false,
			"protocol": "inspector",
			"runtimeExecutable": "npm",
			"windows": {
				"runtimeExecutable": "npm.cmd"
			},
			"runtimeArgs": [ "run-script", "start" ],
			"port": 9229,
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
    }
  ]
}
