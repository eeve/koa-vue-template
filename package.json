{
  "name": "koa-vue-template",
  "version": "0.0.1",
  "description": "koa-vue-template",
  "main": "dist/app.js",
  "author": "eeve",
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/.bin/babel-node --inspect src/app.js",
    "build:web": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/babel-node ./node_modules/webpack/bin/webpack --config ./build/webpack.config.js --progress --hide-modules",
    "build:node": "rimraf dist && cross-env BABEL_ENV=server NODE_ENV=production babel src -d dist --copy-files && rm -rf dist/www/src/ && rm -rf dist/www/dist/",
    "build": "npm run build:node && npm run build:web",
    "start": "NODE_ENV=production node app.js"
  },
  "dependencies": {
    "koa": "^2.8.1",
    "koa-favicon": "^2.0.1",
    "koa-pug": "^4.0.2",
    "koa-static": "^5.0.0",
    "pug": "^2.0.4",
    "too-logger": "^0.0.1-9"
  },
  "devDependencies": {
		"cross-env": "^5.2.1",
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/polyfill": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.0",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.2.1",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "koa-webpack": "^5.2.4",
    "koa2-connect-history-api-fallback": "^0.1.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "rimraf": "^3.0.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^2.1.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-router": "^3.1.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.1"
  }
}
